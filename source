local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("kyo private hub v1.0",  "DarkTheme")
local Version = "0.0.3"

local HomeTab = Window:NewTab("Home")
local HomeSection = HomeTab:NewSection("Home")

HomeSection:NewLabel("Welcome, " .. game.Players.LocalPlayer.Name .. "to Kyos Hub")
HomeSection:NewLabel("Version is " .. Version .. ".")

local UniversalTab = Window:NewTab("Misc")
local UniversalSection1 = UniversalTab:NewSection("Charackter")

UniversalSection1:NewSlider("Walk Speed", "Changes ur speed.", 500, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

UniversalSection1:NewToggle("Toggle Speed", "Turns off Walk Speed", function(state)
    
    if state then
        game.Players.LocalPlayer.Character.Humanoid.UseWalkSpeed = true
    else
        game.Players.LocalPlayer.Character.Humanoid.UseWalkSpeed = false
    end
end)

UniversalSection1:NewSlider("Jump Power", "Changes ur Jump Power.", 500, 50, function(jp)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = jp
end)

UniversalSection1:NewToggle("Toggle Power", "Toggles using Jump Power", function(state)
    
    if state then
        game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = true
    else
        game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = false
    end
end)

HomeSection:NewLabel("made by kyo#2222")

    -- MAIN 
    local UniversalTab = Window:NewTab("Main")
    local UniversalSection1 = UniversalTab:NewSection("Commands")
